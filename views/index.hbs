<table class="table table-bordered">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col" class="text-end">Action</th>
    </tr>
  </thead>
  <tbody>
    {{#each rows}} 
    <tr>
      <th scope="row">1</th>{{this.id}}
      <td>{{this.First_Name}}</td>
      <td>{{this.Last_Name}}</td>
      <td>{{this.email}}</td>
      <td>{{this.phone}}</td>
      <td class="text-end">
        <a href="#" type="button" class="btn btn-light btw-small"><i class="bi bi-eye"></i>View</a>
        <a href="#" type="button" class="btn btn-light btw-small"><i class="bi bi-pencil"></i>Edit</a>
        <a href="#" type="button" class="btn btn-light btw-small"><i class="bi bi-person-x"></i>Delete</a>
      </td>
    </tr>
    {{/each}}




  </tbody>
</table>